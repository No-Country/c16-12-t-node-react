{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/assets/*": ["./src/assets/*"],
      "@/components/*": ["./src/components/*"],
      "@/config/*": ["./src/config/*"],
      "@/pages/*": ["./src/pages/*"],
      "@/api/*": ["./src/providers/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "constants/*": ["./src/constants/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@context/*": ["./src/context/*"],
      "@routes/*": ["./src/routes/*"]
    }
  }
}
